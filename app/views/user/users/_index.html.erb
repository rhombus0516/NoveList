<table>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= link_to user_path(user) do %>
                        <%= image_tag user.get_profile_image(100,100) %>
                    <% end %>
                </td>
                <td><%= user.name %></td>
                <td><%= link_to "退会させる", user_path(user), method: :delete, "data-confirm" => "本当に退会させますか？", class: "btn-sm btn-danger" %></td>
            </tr>
        <% end %>
    </tbody>
</table>

